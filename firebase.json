
{
  "database": {
  "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist/DirectoryVKRT",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "/public/**",
      "destination": "/public.html"
    },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "storage": {
      "rules": "storage.rules"
    }
  }
}
